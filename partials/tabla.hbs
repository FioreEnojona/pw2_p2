<div class="tabla">
    <title>Primera División de Honduras 2024-25 Clausura</title>
    <table>
        <caption>Primera División de Honduras 2024-25 Clausura</caption>
        <thead>
            <tr>
                <th>Pos</th>
                <th>Equipo</th>
                <th>J</th>
                <th>G</th>
                <th>E</th>
                <th>P</th>
                <th>GF</th>
                <th>GC</th>
                <th>DIF</th>
                <th>PTS</th>
            </tr>
        </thead>
        <tbody>
            {{#each tabledata}}
                <tr>
                    <td>{{pos}}</td>
                    <td>{{equipo}}</td>
                    <td>{{jugados}}</td>
                    <td>{{ganados}}</td>
                    <td>{{empatados}}</td>
                    <td>{{perdidos}}</td>
                    <td>{{golesFavor}}</td>
                    <td>{{golesContra}}</td>
                    <td>{{diferencia}}</td>
                    <td>{{puntos}}</td>
                </tr>
            {{/each}}
        </tbody>
    </table>
</div>
